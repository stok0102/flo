<div class="notice">
  <%= flash[:notice] %>
</div>
<% if @current_user %>
  <%= link_to "LOG OUT", log_out_path %>
<% else %>
  <%= link_to "LOG IN", log_in_path %>
  <%= link_to "Sign Up", new_user_path %>
<% end %>

<ul>
  <% @all_questions.each do |question| %>
  <li><%= link_to question.title, user_question_path(question.user, question) %></li>
  <% end %>
</ul>
<% if @current_user %>
  <%= link_to "New Question", new_user_question_path(@current_user) %>
<% end %>
